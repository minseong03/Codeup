#include <stdio.h>
 
int main() {
 
    long long int i;
 
    scanf("%lld", &i);
    
    if (i >= 90 & i<= 100) {
        printf("A");
    }
    else if (i >= 79 && i <= 89) {
        printf("B");
    }
    else if (i >= 40 && i <= 69) {
        printf("C");
    }
    else if (i >= 0 && i <= 39) {
        printf("D");
    }
    return 0;
}
